{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "declaration": true,
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly": true,
    "noImplicitAny": false,
    "allowJs": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [],
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.vitest.json"
    }
  ]
}
